@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Home>
@using ContentModels = Umbraco.Web.PublishedModels;
<form action="/search/">
    <input type="text" class="search-field keyword" name="Keyword" placeholder="Keywords">
    <input type="text" class="search-field location" name="Postcode" placeholder="Postcode">
    <input type="text" class="search-field item-type" name="ItemType" placeholder="Item Type">
    <button class="search-btn" type="button">Search</button>
</form>
